<script>
    function monthsUpdate()
    {
        var months = document.getElementById("months").value;
        var total = document.getElementById("total").value;
        var discount = document.getElementById("discount").value;
        var monthly_price = document.getElementById("monthly_price").value;

        total = monthly_price * months;
        var mrp = (total + ((total * discount)/100));

        document.getElementById("price-text").innerText="₹"+total;
        document.getElementById("mrp-text").innerText="₹"+mrp;
        document.getElementById("total-text").innerText="₹"+total;
        document.getElementById("total").value=total;
    }
</script>